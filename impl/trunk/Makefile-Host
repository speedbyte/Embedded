# Invoke this file by using the command make -f Makefile-Host MAKECMDGOALS=host all-host

# Default target.
all-host: begin makeversion gccversion product end


begin:
	@echo
	@echo "----------------- geht los f√ºr host ----------------------"

end:
	@echo
	@echo "----------------- fertig host----------------------"

# Display compiler version information.
gccversion : 
	@$(CC) --version
	
# Display make version information.
makeversion : 
	@make --version


product:MATLAB HOST

HOST:FORCE
	cd host; make all-host

MATLAB:FORCE
	cd matlab; make all-host

FORCE:

clean-host:
	cd matlab; make clean-host
	cd host; make clean-host
	